@PACKAGE_INIT@

include(${CMAKE_CURRENT_LIST_DIR}/libromanoTargets.cmake)

if(EXISTS "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@")
    set_target_properties(
        @PROJECT_NAME@::@PROJECT_NAME@ PROPERTIES
        IMPORTED_LOCATION_DEBUG "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        IMPORTED_LOCATION_RELEASE "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        IMPORTED_IMPLIB_DEBUG "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@/@CMAKE_IMPORT_LIBRARY_PREFIX@@PROJECT_NAME@@CMAKE_IMPORT_LIBRARY_SUFFIX@"
        IMPORTED_IMPLIB_RELEASE "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@/@CMAKE_IMPORT_LIBRARY_PREFIX@@PROJECT_NAME@@CMAKE_IMPORT_LIBRARY_SUFFIX@"
    )
endif()

set_and_check(libromano_INCLUDE_DIR "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@")
set(libromano_LIBRARIES libromano::libromano)
check_required_components(libromano)